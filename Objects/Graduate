package objects;

public class Graduate extends Student {
	private String major;
	
	public Graduate(String nm, int id, String maj) {
		setName("nm");
		setID(id);
		major = maj;
	}
	
	public Graduate() {
		super();
		setName("");
		setID(0);
		major = "";
	}
	
	public void setMajor(String maj) {
		major = maj;
	}
	
	public String getMajor() {
		return major;
	}
	
	public String print() {
		return "Name: " + getName() + ", " + "ID: " + getID() + ", " + ", " + "Major: " + major;
	}
}
